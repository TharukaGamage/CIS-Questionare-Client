<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.ControllerAddAgeGroup">
   <children>
      <AnchorPane layoutX="400.0" prefHeight="401.0" prefWidth="200.0" style="-fx-background-color: #EFEFEF;">
         <children>
            <ProgressBar layoutX="14.0" layoutY="70.0" prefHeight="18.0" prefWidth="173.0" progress="0.0" />
            <Label layoutX="14.0" layoutY="43.0" text="0% Completed">
               <font>
                  <Font name="Roboto Light" size="16.0" />
               </font>
            </Label>
            <Label layoutX="14.0" layoutY="107.0" text="Answerd">
               <font>
                  <Font name="Roboto Light" size="16.0" />
               </font>
            </Label>
            <Label layoutX="10.0" layoutY="132.0" text="0">
               <font>
                  <Font name="Roboto Light" size="16.0" />
               </font>
            </Label>
            <Label layoutX="14.0" layoutY="207.0" text="Left">
               <font>
                  <Font name="Roboto Light" size="16.0" />
               </font>
            </Label>
            <Label layoutX="14.0" layoutY="229.0" text="8">
               <font>
                  <Font name="Roboto Light" size="16.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
      <Label layoutX="25.0" layoutY="29.0" text="Tell us your age group">
         <font>
            <Font name="Roboto Light" size="24.0" />
         </font>
      </Label>
      <Button fx:id="btnNextAgeGroup" layoutX="259.0" layoutY="327.0" mnemonicParsing="false" onMouseClicked="#btnNextAgeGroup" prefHeight="43.0" prefWidth="113.0" style="-fx-background-color: #97F1FF; -fx-background-radius: 26;" text="NEXT">
         <font>
            <Font name="Roboto Thin" size="19.0" />
         </font></Button>
      <VBox layoutX="25.0" layoutY="80.0" prefHeight="294.0" prefWidth="228.0" spacing="12.0">
