            <RadioButton fx:id="age25" mnemonicParsing="false" text="18 - 25">
               <font>
                  <Font name="Roboto Thin" size="12.0" />
               </font>
               <toggleGroup>
                  <ToggleGroup fx:id="ageGroup" />
               </toggleGroup>
            </RadioButton>
            <RadioButton fx:id="age35" mnemonicParsing="false" text="25 - 35" toggleGroup="$ageGroup">
               <font>
                  <Font name="Roboto Thin" size="12.0" />
