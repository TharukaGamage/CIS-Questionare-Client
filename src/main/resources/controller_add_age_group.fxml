            </RadioButton>
            <RadioButton fx:id="age45" mnemonicParsing="false" text="35 - 45" toggleGroup="$ageGroup">
               <font>
                  <Font name="Roboto Thin" size="12.0" />
               </font>
            </RadioButton>
            <RadioButton fx:id="age55" mnemonicParsing="false" text="45 - 55" toggleGroup="$ageGroup">
               <font>
                  <Font name="Roboto Thin" size="12.0" />
               </font>
            </RadioButton>
            <RadioButton fx:id="age65" mnemonicParsing="false" text="55 - 65" toggleGroup="$ageGroup">
               <font>
                  <Font name="Roboto Thin" size="12.0" />
               </font>
            </RadioButton>
         </children>
      </VBox>
   </children>
